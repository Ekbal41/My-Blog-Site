<div class="row mb-5">
  <div class="col-md-4 col-lg-2 text-uppercase text-muted px-md-5 pt-2 mb-4 mb-md-0">Skills</div>
  <div class="col-md-8 pt-1">
    {% for skill in skills %}
      <div class="mb-4">
        <p class="text-decoration-none text-dark fs-5 fw-semibold m-0">
          {{skill.title}}
        </p>
        <p class="fw-light fs-18 mt-2">{{skill.techs}}</p>
      </div>
    {% endfor %}

  </div>
</div>