<div class="row mb-5">
  <div
    class="col col-md-4 col-lg-3 col-xl-2 text-uppercase text-muted px-md-5 pt-2 mb-4 mb-md-0"
  >
    Projects
  </div>
  <div class="col-md-8 pt-1">

    {% for project in projects %}
      <div class="mb-4">
        <a class="text-dark fs-5 fw-semibold link" href="{{project.url}}">{{project.title}}</a>
        <p class="fw-light fs-18 mt-2">
          {{project.desc}}
        </p>
      </div>
    {% endfor %}

  </div>
</div>